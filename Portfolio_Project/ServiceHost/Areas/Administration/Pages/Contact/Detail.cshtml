@model ContactManagement.Application.Contracts.Contact.ContactDetail


<div class="modal-header">
    <h5 class="modal-title" id="EditModalLabel">Edit @Model.Name</h5>
    <button type="button" class="close ml-1" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">×</span>
    </button>
</div>

<div class="container-fluid pt-4 px-4">
    <div class="row bg-secondary rounded align-items-center justify-content-center mx-0">

        <table class="able table-hover m-5">
            <thead>
                <tr>
                    <th scope="col" class="text-center">#</th>
                    <th scope="col" class="text-center">Email</th>
                    <th scope="col" class="text-center">Message</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row" class="text-center">@Model.Id</th>
                    <td class="text-center">@Model.Email</td>
                    <td class="text-center">@Model.Message</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-danger ml-2" data-dismiss="modal">Close</button>
</div>






